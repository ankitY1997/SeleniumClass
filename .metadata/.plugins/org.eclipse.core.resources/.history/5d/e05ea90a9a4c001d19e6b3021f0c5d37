package StringHandling;

public class Array2 
{
//herewe will se the diiferent way of intilaization 
	// now lets see
	
	public static void main(String []args)
	{
		
	int[] a=new int[3];
		a[0]=10;
		a[1]=20;
		a[2]=30;
		
		for(int i:a)
		{
		System.out.println(i);// if you want to use for each loop so u can use here array variable directly transfer value i
		}
		
		for(int i=0;i<=a.length-1;i++)
		{
			System.out.println(a[i]);
		}
		
		//2.way to initialization
		
		//String []c= {"ankit","rajiv","girish"};
		String c="ankit yadav";
		
		for(int i=0;i<=c.length()-1;i++)
		{
			System.out.println(c.charAt(i));
		}
		
		
		
		
		
	}
	
}
