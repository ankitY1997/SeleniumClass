package methodOverLoading;
import java.util.Scanner;
public class Method_over_3 
{
	//this the example of one class has a multiple main method but you need to remember method parameter will not be same if its same than method overloading will not be there
	//and jvm always read first main(String []args)method
	static int b;
	static int d;
	public static void main(String []args)
	{
		Scanner sc=new Scanner(System.in);
		String c=sc.next();
		String d=sc.nextLine();
		if(c==c)
		{
			main(c);
		}
		else if(d==d)
		{
			main(d);
		}
		
	}
	public static void main(String n)
	{
		System.out.println(n);
	}
	public static void main(char c,int b,int d)
	{
		char a=c;
			b=b;
			d=d;
		if(c==0)
		{
			System.out.println(0);
		}
		else if(c=='+')
		{
			System.out.println("the sum of this :"+(d+a+b));
		}
		else if(c=='*')
		{
			System.out.println("the multification is :"+(d+a+b));
		}
		else if(c=='/')
		{
			System.out.println("the multification is :"+(d+a+b));	
		}
		else if(c=='-')
		{
			System.out.println("the multification is :"+(d+a+b));
		}
	}
	public static void main(char c)
	{
		int d='c';
		System.out.println(d);
			
	}
	public static void main()
	{
		
	}
}
