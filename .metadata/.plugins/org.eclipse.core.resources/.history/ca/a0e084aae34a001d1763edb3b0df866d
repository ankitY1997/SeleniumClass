package StringHandling;

public class Recursion 
{
static int n=0;
	public static void main(String []args)
	{	
	fact(0);
	}
	public static int fact(int n)
	{
	int x=50;
	
	if((x)%n==0)
	{
		System.out.println(n);
	}
	
	
		if(n<=10)
		{
			fact(n+1);
			System.out.println(n);
		}
		
		
		//System.out.println(n);

		return n;
	}
}
