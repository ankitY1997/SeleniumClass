package StringHandling;

public class Duplicate 
{
public static void main(String []args)
{
	int[] arr = {1,2,2,2,3};
	//result array = {1,3}
	int count=0;
	
 // System.out.println(arr.length);
	// tikna 
	
	for(int i=0, j=0;i<=arr.length-1;i++)
	{
	
	
		int c=arr[i];
		//System.out.println(c);
		
		int d=arr[j];
		//System.out.println(d);
		
		count++;
		
		if(c==d)
		{
			//System.out.println(d);
		System.out.println(c);
		
		}
		else if(c!=d)
			{
			//System.out.println(c);
			
			}
		if(count==5)
		{
		j++;	
		}
		
		
	}
}
}
