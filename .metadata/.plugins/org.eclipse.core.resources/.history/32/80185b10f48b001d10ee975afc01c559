package MultiThreading;

public class CarDriver {

	public static void main(String[] args) {
		
	//if you want to know the thread name	System.out.println("thread name is :=>"+Thread.currentThread().getName());

		
		Car c1=new Car("BMW","black",856458);
		Car c2=new Car("Audi","white",1456478);
		Car c3=new Car("Lamborgini","Red",156458);
		
		//now i wan to set priority
		
		c1.start();
		
		for(int i=0;i<=10;i++)
		{
			System.out.println("main thread");
		}
		
		
	}

}
