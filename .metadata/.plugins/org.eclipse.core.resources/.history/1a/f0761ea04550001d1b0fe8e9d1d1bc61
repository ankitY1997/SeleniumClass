package ArrayPractice;

public class Question6 
{
//wajp print the those array which sum is greater than 5
	
	public static void main(String []args)
	{
		int a[]= {20,502,1202,1202,123};
		
		for(int i=0;i<a.length;i++)
		{
			int c=a[i];
			
			while(c!=0)
			{
				int digit=c%10;
				int sum=+digit;
				
			}
		}
	}
}
