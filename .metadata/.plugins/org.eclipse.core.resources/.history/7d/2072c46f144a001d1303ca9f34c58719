package StringHandling;

public class Duplicate 
{
public static void main(String []args)
{
	int[] arr = {1,2,2,2,3};
	//result array = {1,3}
	
  System.out.println(arr.length);
	// tikna 
	
	for(int i=0, j=0,x=0;i<arr.length-1;i++,j++,x++)
	{
	
	
		int c=arr[i];
		int a=arr[j];

		
		if(c==a)
		{
			//System.out.println(c);
		}
	
		System.out.println(c);	
		
		
	}
}
}
