package Practice;

public class Program1 {

	public static void main(String[] args) 
	{
		
		String x="anbcdgdgdh";
		String y=x;
		int count =0;
		boolean flag=false;
		int num=x.length();
		System.out.println(num);
	
		int i=0;
		int j=0;
		while(i<x.length())
		{
		
			char z=x.charAt(i);
			char l=y.charAt(j);
			
			System.out.println(z+"==="+l);
			
			i++;
			j++;
			
			
		}
	}
	
	

}
