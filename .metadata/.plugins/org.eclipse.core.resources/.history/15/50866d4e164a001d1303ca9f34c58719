package StringHandling;

public class Duplicate 
{
public static void main(String []args)
{
	int[] arr = {1,2,2,2,3};
	//result array = {1,3}
	
 // System.out.println(arr.length);
	// tikna 
	
	for(int i=0, j=5,x=0;i<=arr.length-1;i++,j--,x++)
	{
	
	
		int c=arr[i];
		int a=arr[j];
			//System.out.println(a);
			//System.out.println(c);
			
		
		if(c==a)
		{
			//System.out.println(c);
		}
	
		else if(c!=a)
		{
		System.out.println(c);
		}
		
		
	}
}
}
