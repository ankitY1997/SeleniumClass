package methodOverLoading;

public class Shoes1 
{
public static void main(String []args)
{
	main("gold star","white",8,300,"school shoes");
}
public static void main(String n,String c,int size,int p,String v)
{
	if((n=="gold star")&&((c=="white")||(c=="black"))&&(size==8)&&(p==300)&&(v=="school shoes"))
	{
		if(c=="white")
		{
		System.out.println("model name=123456");
		}
	}
}
}
