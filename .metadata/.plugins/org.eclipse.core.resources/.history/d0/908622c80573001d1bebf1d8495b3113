package Practice;

public class Program1 {

	public static void main(String[] args) 
	{
		
		String x="anbcdgdgdh";
		String y=x;
		int count =0;
		boolean flag=false;
		int num=x.length();
		System.out.println(num);
		
	for( int i=0,j=0;i<x.length();j++)
		{
			char c=x.charAt(i);
			
		
			char m=y.charAt(j);
			
			//System.out.println(c);
			
			
			if((m>c)&&(flag==true))
			{
				System.out.println(m);
				
			}
			else if(m==x.length()-1)
			{
			
			j=0;
			
				
			i++;	
			}
			else {
				flag=true;
			}
			
			


		}
		
	}
	
	

}
