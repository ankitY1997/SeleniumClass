package MultiThreading;

public class Car extends Thread{
	
	
	//create a thread by extends thread class in our class
	
	
	String name="Audi";
	String color="black";
    int price=58765;
    
    
    public Car(String name,String color,int price)
    {
    	this.name=name;
    	this.color=color;
    	this.price=price;
    }
    
    public void display()
    {
    	for(int i=0;i<=10;i++)
    	{
    		System.out.println("child thread");
    	}
    }
    @Override
    public void run()
    {
  
    	for(int i=0;i<=10;i++)
    	{
    		System.out.println("child thread");
    	}
    	
    }
    
    

}
