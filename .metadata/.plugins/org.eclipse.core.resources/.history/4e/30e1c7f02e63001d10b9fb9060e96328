package Abstratction;

abstract class Viechle 
{
	public static final String name="this ankit";
	
	//this is abstract class
	public abstract void name();
	public abstract void colour();
	
	//this is concrete class
	
	//concrete class will also have body
	
	public static void gear()
	{
	System.out.println("this is gear method inside viechle class");
	}
	
	
	
	
	
}
