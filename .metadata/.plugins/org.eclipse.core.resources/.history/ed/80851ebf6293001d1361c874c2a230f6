package com.flipkart.utils.testcase;

public class Assigmnent1 {
	
	
	public static int  totalstr;
	public static void main(String[] args) {
		String []a= {"ab","bc","ajnd",};
		method2(a);
		
	}
	
	public static void method2(String [] s)
	{
		totalstr=s.length-1;
		
		
		if(totalstr>=0)
		{
			
			System.out.println(s[totalstr]);
			
		}
		
		 
		 
		method2(s);
		
		
	}

}
