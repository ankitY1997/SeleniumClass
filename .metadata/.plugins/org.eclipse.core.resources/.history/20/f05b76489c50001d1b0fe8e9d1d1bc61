package ArrayPractice;

public class Question7 
{
//wajp to print the array element only if its prime number
	public static void main(String []args)
	{
		int a[]= {20,35,31,13,17};
		int count=0;
		int i=1;
		int j=0;
		while(j<a.length)
		{
			int c=a[j];
		while(i<c/2)
		 {
			if(c%i==0)
			{
				count++;
				
			}
			i++;
		 }
		     if(count==1)
		      {
			System.out.println(c);
		      }
		count=0;
		j++;	
		}
	}
}
