package StringHandling;

public class Duplicate 
{
public static void main(String []args)
{
	int[] arr = {1,2,2,2,3};
	//result array = {1,3}
	
  System.out.println(arr.length);
	// tikna 
	
	for(int i=0, j=arr.length;i<arr.length-1;i++,j--)
	{
	
	/*char c=s.charAt(i);
	char a=s.charAt(j);*/
		int c=arr[i];
		int a=arr[j];
	if(c==a)
	{
		//System.out.println();
		
	}
	else if(c!=a)
	{
	System.out.println(a);
	}
	}
}
}
