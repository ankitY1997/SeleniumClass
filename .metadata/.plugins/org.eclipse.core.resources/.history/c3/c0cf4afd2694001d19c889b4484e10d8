package Annotation;

import org.testng.annotations.Ignore;
import org.testng.annotations.Test;

public class IgnoreAnnotation 
{

	
	/*how to ignore a test case =>we have @Ignore annotation with this annotation we  
	 * can ignore test method also class and also package.
	*/
	
	@Test
	public void test1()
	{
		System.out.println("test-1");
	}
	@Test
	public void test2()
	{
		System.out.println("test-2");
	}
	
	//see here this test case is not running 
	@Ignore
	@Test
	public void test3()
	{
		System.out.println("test-3");
	}
}
